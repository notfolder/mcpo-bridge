{
  "mcpServers": {
    "powerpoint": {
      "command": "npx",
      "args": [
        "-y",
        "@gongrzhe/office-powerpoint-mcp-server"
      ],
      "env": {
        "NODE_ENV": "production"
      }
    },
    "excel": {
      "command": "uvx",
      "args": [
        "excel-mcp-server",
        "stdio"
      ]
    }
  }
}
